// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Patient {
  id        String   @id @default(uuid())
  name      String
  dateBirth DateTime @map("date_birth")
  genre     String
  cpf       String
  cellphone BigInt
  address   String

  @@map("patients")
}
